    script.
      $('#uri').keypress(function (e) {
      var key = e.which;
      if(key == 13)  // the enter key code
      {
      var $iframe = $('#song');
      if ( $iframe.length ) {
      var unfiltered = $('#uri').val();
      var filtered = unfiltered.substring(unfiltered.indexOf('=') + 1 , unfiltered.length);
      console.log(filtered);
      $iframe.attr('src','https://www.youtube.com/embed/'+ filtered + '?autoplay=1');
      $('#uri').val('');
      return false;
      }
      return true;
      return false;
      }
